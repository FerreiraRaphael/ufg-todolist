{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=express-sequelize node ./bin/www",
    "dev": "cross-env DEBUG=express-sequelize node --inspect ./bin/www",
    "test": "cross-env NODE_ENV=test mocha routes/*.spec.js",
    "test:debug": "cross-env NODE_ENV=test mocha debug routes/*.spec.js",
    "test:coverage":
      "cross-env NODE_ENV=test node_modules/.bin/istanbul cover -x *.spec.js --report html node_modules/mocha/bin/_mocha -- -R spec routes/*.spec.js",
    "report:coverage": "cross-env NODE_ENV=test istanbul report cobertura"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.2.0",
    "express": "^4.14.0",
    "istanbul": "1.1.0-alpha.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.0",
    "mocha-junit-reporter": "^1.15.0",
    "morgan": "^1.7.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^6.1.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.3.0",
    "spec-xunit-file": "0.0.1-3",
    "sqlite3": "^3.0.8"
  },
  "devDependencies": {
    "bluebird": "^3.4.1",
    "cross-env": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "supertest": "^2.0.0"
  }
}
